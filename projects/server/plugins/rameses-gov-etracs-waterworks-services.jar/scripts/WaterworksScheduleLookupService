import com.rameses.annotations.*;
import com.rameses.util.*;
import java.rmi.server.*;
import com.rameses.common.*;
import com.rameses.services.extended.*;

class WaterworksScheduleLookupService extends ActiveListService{

	@ActiveDB(value='waterworks_schedule', em="waterworks")
	def em;

	String listMethod = 'getScheduleLookup';

	@ProxyMethod
	def getColumns(params){
		return [
			[name:'year', caption:'Year'],
			[name:'month', caption:'Month'],
			[name:'duedate', caption:'Due Date'],
			[name:'cuttingdate', caption:'Cutting Date']
		];
	}

	void beforeList(params){
		params.searchtext = params.searchtext ? params.searchtext : '%';
	}
}