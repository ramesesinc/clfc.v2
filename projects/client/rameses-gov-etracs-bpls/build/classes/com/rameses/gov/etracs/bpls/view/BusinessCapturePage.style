"lob.listModel.item.oldname" [capturetype=='manual'] {
    visible:false;
}
"lob.listModel.item.oldname" [capturetype!='manual'] {
    visible:true;
}
"entity.(taskstate|dtfiled)" [entity.state == 'PROCESSING'] {
    visible:true;
}
"entity.(taskstate|dtfiled)" [entity.state != 'PROCESSING'] {
    visible:false;
}