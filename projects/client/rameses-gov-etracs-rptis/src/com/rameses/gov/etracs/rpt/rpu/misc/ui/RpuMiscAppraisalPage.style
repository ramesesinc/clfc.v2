"entity.rpu.*" [allowEdit == true]{
    enabled:true;
}

"selectedItem|selectedParam" [allowEdit == true]{
    readonly:false;
}


"entity.rpu.*" [allowEdit != true]{
    enabled:false;
}

"selectedItem|selectedParam" [allowEdit != true]{
    readonly:true;
}

"selectedParam" [selectedItem == null]{
    readonly:true;
}